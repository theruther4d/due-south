<div class="slider">
    <div class="slider__wrapper">
        <% slice.value.value.forEach( function( slide ) { %>
            <div class="slider__slide">
                <img class="slider__slide__image fluid" src="<%- makeImgix( slide.fragments.image.url ) %>" />
            </div>
        <% }); %>
    </div>
</div>
